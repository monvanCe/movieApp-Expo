{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@const/*": ["const/*"],
      "@HOCs/*": ["HOCs/*"],
      "@hooks/*": ["hooks/*"],
      "@layout/*": ["layout/*"],
      "@localization/*": ["localization/*"],
      "@middleware/*": ["middleware/*"],
      "@service/*": ["service/*"],
      "@store/*": ["store/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@assets/*": ["assets/*"]
    }
  }
}
